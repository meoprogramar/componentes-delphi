unit SolidLongInput;

interface

uses
  System.SysUtils, System.Classes, FMX.Types, FMX.Controls, LongInput;

type
  TSolidLongInput = class(TLongInput)
  private
    { Private declarations }
  protected
    { Protected declarations }
  public
    { Public declarations }
  published
    { Published declarations }
  end;

procedure Register;

implementation

procedure Register;
begin
  RegisterComponents('Componentes Customizados', [TSolidLongInput]);
end;

end.
